{
  "manifest_version": 3,
  "name": "phishing-detection",
  "version": "1.0.0",
  "description": "Chặn truy cập các URL phishing dựa trên database nội bộ.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://localhost/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  
  "action": {
    "default_title": "Phishing Detection"
  },
  "web_accessible_resources": [
    {
      "resources": ["blocked.html", "blocked.js", "styles.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html"
}
